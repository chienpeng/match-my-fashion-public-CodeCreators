				<div class="alert style-3 alert-dismissible">
					<a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
				    <strong><%= flash[:notice]%></strong>
				</div>
			</div>
			<% elsif flash[:success] %>
